{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chimango Server",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/server/app.ts",
            "stopOnEntry": false,
            "args": [],
            "runtimeExecutable": null,
            "runtimeArgs": ["--nolazy"],
            "env": {
                "NODE_ENV": "development"
            },
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/built"
        },
        {
            "name": "Attach to Node",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/built",
            "stopOnEntry": true
        }
    ]
}